{
  "name": "ioniclib",
  "version": "4.0.3",
  "description": "Ionic Common Library",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "keywords": [
    "angular",
    "angular4",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "ionic",
    "library",
    "ng",
    "typescript",
    "typing",
    "typestub",
    "common"
  ],
  "author": "Beeno Tung (https://www.facebook.com/messages/t/beenotung)",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/angularlib.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/angularlib/issues"
  },
  "homepage": "https://github.com/beenotung/angularlib#readme",
  "scripts-info": {
    "tslint:aot": "for debug only. not recommented, since the output maybe very compact"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run lint && npm run test && npm run build && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "npm run dist:unmove && rimraf coverage dist tmp docs www",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage --disablePrivateOrInternalSupport",
    "explorer": "source-map-explorer ./dist/index.umd.js",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "lint": "npm run tslint",
    "ngcompile": "ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci && npm run dist:move",
    "preversion": "npm run ci",
    "predist:move": "npm run dist:unmove",
    "dist:move": "./dist:move",
    "dist:unmove": "./dist:unmove",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "tslint": "tslint --type-check --project tsconfig.json",
    "tslint:aot": "tslint --type-check --project tsconfig-aot.json",
    "ionic:clean": "ionic-app-scripts clean --tsconfig tsconfig-ionic.json",
    "ionic:build": "ionic-app-scripts build --tsconfig tsconfig-ionic.json",
    "ionic:lint": "ionic-app-scripts lint --tsconfig tsconfig-ionic.json",
    "ionic:serve": "ionic-app-scripts serve --tsconfig tsconfig-ionic.json"
  },
  "dependencies": {
    "@angular/common": "4.4.3",
    "@angular/compiler": "4.4.3",
    "@angular/compiler-cli": "4.4.3",
    "@angular/core": "4.4.3",
    "@angular/forms": "4.4.3",
    "@angular/http": "4.4.3",
    "@angular/platform-browser": "4.4.3",
    "@angular/platform-browser-dynamic": "4.4.3",
    "@beenotung/tslib": "^7.0.3",
    "ionic-angular": "3.7.1",
    "rxjs": "^5.4.3",
    "zone.js": "0.8.18"
  },
  "peerDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/core": "^4.0.0",
    "rxjs": "^5.4.0",
    "typestub-horizon-client": "^0.1.7"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.2",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@ionic/app-scripts": "3.0.0",
    "@types/jasmine": "^2.5.47",
    "@types/karma": "^0.13.35",
    "@types/node": "^7.0.18",
    "@types/webpack": "^2.2.15",
    "@types/webpack-env": "^1.13.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "codecov": "^2.2.0",
    "codelyzer": "^3.0.1",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.1",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.1",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.2",
    "raw-loader": "^0.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "^6.0.5",
    "source-map-explorer": "^1.3.3",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "2.3.4",
    "typestub-horizon-client": "^0.1.7",
    "webpack": "^3.1.0",
    "webpack-angular-externals": "^1.0.2",
    "webpack-rxjs-externals": "^1.0.0"
  }
}
